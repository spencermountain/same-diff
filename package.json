{
  "name": "same-diff",
  "version": "0.0.1",
  "description": "render text diffs similarly in different contexts",
  "homepage": "https://github.com/spencermountain/same-diff#readme",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
